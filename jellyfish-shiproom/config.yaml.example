# Google Slides Configuration
google_slides:
  presentation_id: "your-google-slides-presentation-id"
  # slide_id is now configured per team in teams_config.yaml

# Teams Configuration
teams_config_file: "teams_config.yaml"

# Team Selection
# Use "all" to generate slides for all teams, or specify a team identifier (e.g., "BBEE", "TOL", "CLIP")
team_selection: "all"  # Options: "all" or specific team identifier like "BBEE"

